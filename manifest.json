{
  "manifest_version": 2,
  "name": "New Sonali Personal Post Extractor",
  "description": "Today Quick launch ",
  "version": "0.0.2",
  "icons": { "128": "Salesflags.png" }, 
  "browser_action": {"default_icon": "Salesflags.png", "default_popup":"popup.html"},
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
  

}
